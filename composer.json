{
  "name": "nevermind23/commission-soloutions",
  "description": "Simple commission solutions assignment",
  "keywords": [
    "commission",
    "payment",
    "solution"
  ],
  "type": "project",
  "autoload": {
    "psr-4": {
      "Nevermind23\\CommissionSolutions\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nevermind23\\CommissionSolutions\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "nesbot/carbon": "^2.58"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "friendsofphp/php-cs-fixer": "2.17"
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "scripts": {
    "phpunit": "phpunit",
    "fix-cs": "php-cs-fixer fix",
    "test-cs": "php-cs-fixer fix --dry-run -v",
    "test": [
      "@phpunit",
      "@test-cs"
    ]
  }
}
